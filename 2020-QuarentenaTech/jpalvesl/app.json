{
  "expo": {
    "name": "Jogo da velha",
    "slug": "jogo-da-velha",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#860c4e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gmail.jpalvesl"
    },
    "android": {
      "package": "com.gmail.jpalvesl.jogodavelha"
    }
  }
}
